{
  "name": "ts-streams",
  "version": "0.1.0",
  "description": "A collection util can process list by some operation like flatMap distinct shuffle and reduce. Taking inspiration from java stream",
  "homepage": "https://github.com/VendaCino/tsstream",
  "author": "venda",
  "main": "dist/TsStream.js",
  "umd:main": "dist/umd/tsstream.js",
  "typings": "dist/TsStream.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "keywords": [
    "functional",
    "collection",
    "pipeline",
    "stream",
    "lazy",
    "utils",
    "java",
    "array"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "compile": "tsc",
    "test": "nyc ./node_modules/.bin/_mocha 'test/**/*.ts' --require ./register.js",
    "bundle": "webpack --mode production && size-limit",
    "build": "yarn clean && yarn lint && yarn compile && yarn test && yarn bundle"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/VendaCino/tsstream.git"
  },
  "bugs": {
    "url": "https://github.com/VendaCino/tsstream/issues"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/amqplib": "^0.5.17",
    "@types/chai": "^4.2.16",
    "@types/node": "^15.0.2",
    "awesome-typescript-loader": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "1.10.2",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.2",
    "size-limit": "^0.19.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "size-limit": [
    {
      "path": "dist/umd/tsstream.min.js",
      "limit": "16 KB"
    }
  ]
}
